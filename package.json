    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "serve": "^14.1.2"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "ora": "^6.1.2"
  }
