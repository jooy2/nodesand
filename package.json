{
  "name": "nodesand",
  "version": "1.0.0",
  "description": "Super Simple Node.js Sandbox",
  "private": true,
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "once": "node src/_preload",
    "watch": "nodemon src/_preload",
    "serve": "serve src/www",
    "www": "npm run serve",
    "dev": "npm run once",
    "test": "npm run once",
    "lint": "eslint . --ext .js,.mjs,.cjs,.vue src",
    "format": "prettier .",
    "format:fix": "prettier . --write",
    "reset": "git reset HEAD --hard && git clean -f -d -x -e '.idea/' && npm i"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "CDGet <jooy2.contact@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jooy2/nodesand.git"
  },
  "bugs": {
    "url": "https://github.com/jooy2/nodesand/issues"
  },
  "homepage": "https://github.com/jooy2/nodesand",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-n": "^17.23.1",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "serve": "^14.2.5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "ora": "^9.0.0"
  }
}
